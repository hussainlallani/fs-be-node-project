{
  "name": "fs-be-node-project",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build:ts": "tsc",
    "build:ext": "npx ts-add-js-extension add --dir=dist)",
    "build": "(npm run build:ts && npm run build:ext) || npx ts-add-js-extension add --dir=dist",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:debug": "DEBUG=app:* nodemon src/index.ts",
    "dev:ts": "ts-node-esm src/index.ts",
    "lint": "jshint --config .jshintrc src"
  },
  "dependencies": {
    "debug": "^4.4.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "mongodb": "^6.9.0",
    "morgan": "^1.10.0",
    "rimraf": "^6.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.5",
    "jshint": "^2.13.6",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "ts-add-js-extension": "^1.6.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
